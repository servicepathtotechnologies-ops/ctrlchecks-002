{
  "name": "Test ActiveCampaign - Get Contact",
  "description": "Test workflow for ActiveCampaign CRM node - Get contact operation",
  "nodes": [
    {
      "id": "node_1",
      "type": "custom",
      "position": { "x": 100, "y": 100 },
      "data": {
        "type": "manual_trigger",
        "label": "Manual Trigger",
        "category": "triggers",
        "icon": "Play",
        "config": {}
      }
    },
    {
      "id": "node_2",
      "type": "custom",
      "position": { "x": 300, "y": 100 },
      "data": {
        "type": "set",
        "label": "Set Contact ID",
        "category": "data",
        "icon": "Edit",
        "config": {
          "fields": "{\"contactId\": \"123\"}"
        }
      }
    },
    {
      "id": "node_3",
      "type": "custom",
      "position": { "x": 500, "y": 100 },
      "data": {
        "type": "activecampaign",
        "label": "ActiveCampaign",
        "category": "crm",
        "icon": "Zap",
        "config": {
          "apiKey": "your-api-key",
          "apiUrl": "https://youraccount.api-us1.com",
          "resource": "contact",
          "operation": "get",
          "id": "{{contactId}}"
        }
      }
    },
    {
      "id": "node_4",
      "type": "custom",
      "position": { "x": 700, "y": 100 },
      "data": {
        "type": "log_output",
        "label": "Log Output",
        "category": "output",
        "icon": "FileOutput",
        "config": {
          "message": "ActiveCampaign Contact: {{contact}}",
          "level": "info"
        }
      }
    }
  ],
  "edges": [
    {
      "id": "edge_1",
      "source": "node_1",
      "target": "node_2"
    },
    {
      "id": "edge_2",
      "source": "node_2",
      "target": "node_3"
    },
    {
      "id": "edge_3",
      "source": "node_3",
      "target": "node_4"
    }
  ],
  "viewport": { "x": 0, "y": 0, "zoom": 1 }
}

