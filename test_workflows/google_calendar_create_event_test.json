{
  "name": "Google Calendar - Create Event Test",
  "description": "Test creating a new event in primary calendar with attendees and reminders.",
  "nodes": [
    {
      "id": "node_1",
      "type": "custom",
      "position": { "x": 100, "y": 100 },
      "data": {
        "type": "manual_trigger",
        "label": "Start Test",
        "category": "triggers",
        "icon": "Play",
        "config": {}
      }
    },
    {
      "id": "node_2",
      "type": "custom",
      "position": { "x": 300, "y": 100 },
      "data": {
        "type": "google_calendar",
        "label": "Create Event",
        "category": "productivity",
        "icon": "Calendar",
        "config": {
          "resource": "event",
          "operation": "create",
          "calendarId": "primary",
          "summary": "Team Meeting - Test Event",
          "start": {
            "dateTime": "2025-03-15T10:00:00-07:00",
            "timeZone": "America/Los_Angeles"
          },
          "end": {
            "dateTime": "2025-03-15T11:00:00-07:00",
            "timeZone": "America/Los_Angeles"
          },
          "eventData": {
            "description": "This is a test event created by CtrlChecks workflow",
            "location": "Conference Room A",
            "attendees": [
              {
                "email": "colleague@example.com"
              }
            ],
            "reminders": {
              "useDefault": false,
              "overrides": [
                {
                  "method": "email",
                  "minutes": 15
                },
                {
                  "method": "popup",
                  "minutes": 10
                }
              ]
            }
          },
          "sendUpdates": "all"
        }
      }
    },
    {
      "id": "node_3",
      "type": "custom",
      "position": { "x": 500, "y": 100 },
      "data": {
        "type": "log_output",
        "label": "View Created Event",
        "category": "output",
        "icon": "FileOutput",
        "config": {
          "message": "Event created: {{$json.summary}} (ID: {{$json.id}})",
          "level": "info"
        }
      }
    }
  ],
  "edges": [
    {
      "id": "edge_1",
      "source": "node_1",
      "target": "node_2"
    },
    {
      "id": "edge_2",
      "source": "node_2",
      "target": "node_3"
    }
  ],
  "viewport": { "x": 0, "y": 0, "zoom": 1 }
}
