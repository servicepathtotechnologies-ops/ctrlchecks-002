          "soql": "SELECT Id, Name, Email FROM Contact LIMIT 10"
        }
      }
    },
    {
      "id": "node_3",
      "type": "custom",
      "position": { "x": 500, "y": 100 },
      "data": {
        "type": "log_output",
        "label": "Log Output",
        "category": "output",
        "icon": "FileOutput",
        "config": {
          "message": "Salesforce Results: {{results}}",
          "level": "info"
        }
      }
    }
  ],
  "edges": [
    {
      "id": "edge_1",
      "source": "node_1",
      "target": "node_2"
