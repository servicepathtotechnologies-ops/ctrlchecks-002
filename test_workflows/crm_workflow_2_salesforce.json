{
  "name": "Test Salesforce - Query Contacts",
  "description": "Test workflow for Salesforce CRM node - Query contacts operation",
  "nodes": [
    {
      "id": "node_1",
      "type": "custom",
      "position": { "x": 100, "y": 100 },
      "data": {
        "type": "manual_trigger",
        "label": "Manual Trigger",
        "category": "triggers",
        "icon": "Play",
        "config": {}
      }
    },
    {
      "id": "node_2",
      "type": "custom",
