      "data": {
        "type": "salesforce",
        "label": "Salesforce",
        "category": "crm",
        "icon": "Cloud",
        "config": {
          "instanceUrl": "https://connect-agility-9124.my.salesforce.com",
          "accessToken": "your-oauth-access-token",
