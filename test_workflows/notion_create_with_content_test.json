{
  "name": "Notion Test - Create Page with Content",
  "description": "Create a new child page with rich content (heading, paragraphs). Uses OAuth from header automatically.",
  "nodes": [
    {
      "id": "node_1",
      "type": "custom",
      "position": { "x": 100, "y": 100 },
      "data": {
        "type": "manual_trigger",
        "label": "Start",
        "category": "triggers",
        "icon": "Play",
        "config": {}
      }
    },
    {
      "id": "node_2",
      "type": "custom",
      "position": { "x": 300, "y": 100 },
      "data": {
        "type": "notion",
        "label": "Create Page with Content",
        "category": "database",
        "icon": "Database",
        "config": {
          "resource": "page",
          "operation": "create",
          "parentPageId": "YOUR_PARENT_PAGE_ID_HERE",
          "children": [
            {
              "object": "block",
              "type": "heading_1",
              "heading_1": {
                "rich_text": [{"type": "text", "text": {"content": "New Document"}}]
              }
            },
            {
              "object": "block",
              "type": "paragraph",
              "paragraph": {
                "rich_text": [{"type": "text", "text": {"content": "This is the first paragraph of the new page."}}]
              }
            },
            {
              "object": "block",
              "type": "paragraph",
              "paragraph": {
                "rich_text": [{"type": "text", "text": {"content": "This is another paragraph with more content."}}]
              }
            }
          ]
        }
      }
    },
    {
      "id": "node_3",
      "type": "custom",
      "position": { "x": 500, "y": 100 },
      "data": {
        "type": "log_output",
        "label": "View Created Page",
        "category": "output",
        "icon": "FileOutput",
        "config": {
          "message": "Created page: {{$json.url}}",
          "level": "info"
        }
      }
    }
  ],
  "edges": [
    {
      "id": "edge_1",
      "source": "node_1",
      "target": "node_2"
    },
    {
      "id": "edge_2",
      "source": "node_2",
      "target": "node_3"
    }
  ],
  "viewport": { "x": 0, "y": 0, "zoom": 1 }
}
