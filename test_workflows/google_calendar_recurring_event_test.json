{
  "name": "Google Calendar - Recurring Event Test",
  "description": "Test creating a recurring event (weekly meeting for 10 weeks).",
  "nodes": [
    {
      "id": "node_1",
      "type": "custom",
      "position": { "x": 100, "y": 100 },
      "data": {
        "type": "manual_trigger",
        "label": "Start Test",
        "category": "triggers",
        "icon": "Play",
        "config": {}
      }
    },
    {
      "id": "node_2",
      "type": "custom",
      "position": { "x": 300, "y": 100 },
      "data": {
        "type": "google_calendar",
        "label": "Create Recurring Event",
        "category": "productivity",
        "icon": "Calendar",
        "config": {
          "resource": "event",
          "operation": "create",
          "calendarId": "primary",
          "summary": "Weekly Team Standup",
          "start": {
            "dateTime": "2025-03-17T09:00:00-07:00",
            "timeZone": "America/Los_Angeles"
          },
          "end": {
            "dateTime": "2025-03-17T09:30:00-07:00",
            "timeZone": "America/Los_Angeles"
          },
          "eventData": {
            "description": "Weekly team standup meeting - recurring for 10 weeks",
            "location": "Conference Room B",
            "recurrence": [
              "RRULE:FREQ=WEEKLY;COUNT=10;BYDAY=MO"
            ],
            "reminders": {
              "useDefault": false,
              "overrides": [
                {
                  "method": "popup",
                  "minutes": 5
                }
              ]
            }
          },
          "sendUpdates": "none"
        }
      }
    },
    {
      "id": "node_3",
      "type": "custom",
      "position": { "x": 500, "y": 100 },
      "data": {
        "type": "log_output",
        "label": "View Result",
        "category": "output",
        "icon": "FileOutput",
        "config": {
          "message": "Recurring event created: {{$json.summary}} ({{$json.recurrence.length}} recurrence rules)",
          "level": "info"
        }
      }
    }
  ],
  "edges": [
    {
      "id": "edge_1",
      "source": "node_1",
      "target": "node_2"
    },
    {
      "id": "edge_2",
      "source": "node_2",
      "target": "node_3"
    }
  ],
  "viewport": { "x": 0, "y": 0, "zoom": 1 }
}
