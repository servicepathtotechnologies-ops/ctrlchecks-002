{
  "name": "ClickUp Get Tasks Test",
  "description": "Test workflow to retrieve tasks from a ClickUp list",
  "nodes": [
    {
      "id": "node_1",
      "type": "custom",
      "position": { "x": 100, "y": 100 },
      "data": {
        "type": "manual_trigger",
        "label": "Start",
        "category": "triggers",
        "icon": "Play",
        "config": {}
      }
    },
    {
      "id": "node_2",
      "type": "custom",
      "position": { "x": 300, "y": 100 },
      "data": {
        "type": "clickup",
        "label": "Get Tasks",
        "category": "database",
        "icon": "Database",
        "config": {
          "operation": "get_tasks_list",
          "apiKey": "YOUR_CLICKUP_API_TOKEN_HERE",
          "listId": "YOUR_LIST_ID_HERE",
          "includeClosed": false,
          "page": 0
        }
      }
    },
    {
      "id": "node_3",
      "type": "custom",
      "position": { "x": 500, "y": 100 },
      "data": {
        "type": "log_output",
        "label": "View Tasks",
        "category": "output",
        "icon": "FileOutput",
        "config": {
          "message": "Found {{tasks.length}} tasks in the list",
          "level": "info"
        }
      }
    }
  ],
  "edges": [
    {
      "id": "edge_1",
      "source": "node_1",
      "target": "node_2"
    },
    {
      "id": "edge_2",
      "source": "node_2",
      "target": "node_3"
    }
  ],
  "viewport": { "x": 0, "y": 0, "zoom": 1 }
}
