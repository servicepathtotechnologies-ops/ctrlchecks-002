{
  "name": "Test Intercom - Get Contact",
  "description": "Test workflow for Intercom CRM node - Get contact operation",
  "nodes": [
    {
      "id": "node_1",
      "type": "custom",
      "position": { "x": 100, "y": 100 },
      "data": {
        "type": "manual_trigger",
        "label": "Manual Trigger",
        "category": "triggers",
        "icon": "Play",
        "config": {}
      }
    },
    {
      "id": "node_2",
      "type": "custom",
      "position": { "x": 300, "y": 100 },
      "data": {
        "type": "set",
        "label": "Set Contact ID",
        "category": "data",
        "icon": "Edit",
        "config": {
          "fields": "{\"contactId\": \"123456\"}"
        }
      }
    },
    {
      "id": "node_3",
      "type": "custom",
      "position": { "x": 500, "y": 100 },
      "data": {
        "type": "intercom",
        "label": "Intercom",
        "category": "crm",
        "icon": "MessageCircle",
        "config": {
          "accessToken": "your-access-token",
          "resource": "contact",
          "operation": "get",
          "id": "{{contactId}}"
        }
      }
    },
    {
      "id": "node_4",
      "type": "custom",
      "position": { "x": 700, "y": 100 },
      "data": {
        "type": "log_output",
        "label": "Log Output",
        "category": "output",
        "icon": "FileOutput",
        "config": {
          "message": "Intercom Contact: {{contact}}",
          "level": "info"
        }
      }
    }
  ],
  "edges": [
    {
      "id": "edge_1",
      "source": "node_1",
      "target": "node_2"
    },
    {
      "id": "edge_2",
      "source": "node_2",
      "target": "node_3"
    },
    {
      "id": "edge_3",
      "source": "node_3",
      "target": "node_4"
    }
  ],
  "viewport": { "x": 0, "y": 0, "zoom": 1 }
}

