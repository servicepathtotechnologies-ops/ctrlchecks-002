{
  "name": "Chat Trigger - Function Node Test",
  "description": "Test workflow for chat trigger with Function node - processes message with custom function",
  "nodes": [
    {
      "id": "node_1",
      "type": "custom",
      "position": { "x": 100, "y": 100 },
      "data": {
        "type": "chat_trigger",
        "label": "Chat Trigger",
        "category": "triggers",
        "icon": "MessageSquare",
        "config": {
          "triggerMode": "on_message_received"
        }
      }
    },
    {
      "id": "node_2",
      "type": "custom",
      "position": { "x": 300, "y": 100 },
      "data": {
        "type": "function",
        "label": "Process Message",
        "category": "data",
        "icon": "Code",
        "config": {
          "code": "return {\n  originalMessage: input.message,\n  processedMessage: input.message.toUpperCase(),\n  wordCount: input.message.split(' ').length,\n  timestamp: new Date().toISOString()\n};",
          "timeout": 5000
        }
      }
    },
    {
      "id": "node_3",
      "type": "custom",
      "position": { "x": 500, "y": 100 },
      "data": {
        "type": "log_output",
        "label": "Log Result",
        "category": "output",
        "icon": "FileOutput",
        "config": {
          "message": "Processed: {{input.processedMessage}} | Words: {{input.wordCount}}",
          "level": "info"
        }
      }
    }
  ],
  "edges": [
    {
      "id": "edge_1",
      "source": "node_1",
      "target": "node_2"
    },
    {
      "id": "edge_2",
      "source": "node_2",
      "target": "node_3"
    }
  ],
  "viewport": { "x": 0, "y": 0, "zoom": 1 }
}
