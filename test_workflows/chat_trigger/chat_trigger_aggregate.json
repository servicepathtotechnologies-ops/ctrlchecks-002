{
  "name": "Chat Trigger - Aggregate Node Test",
  "description": "Test workflow for chat trigger with Aggregate node - aggregates array data",
  "nodes": [
    {
      "id": "node_1",
      "type": "custom",
      "position": { "x": 100, "y": 100 },
      "data": {
        "type": "chat_trigger",
        "label": "Chat Trigger",
        "category": "triggers",
        "icon": "MessageSquare",
        "config": {
          "triggerMode": "on_message_received"
        }
      }
    },
    {
      "id": "node_2",
      "type": "custom",
      "position": { "x": 300, "y": 100 },
      "data": {
        "type": "javascript",
        "label": "Create Data",
        "category": "data",
        "icon": "Code",
        "config": {
          "code": "return {\n  items: [\n    {name: \"Item 1\", price: 10, category: \"A\"},\n    {name: \"Item 2\", price: 20, category: \"A\"},\n    {name: \"Item 3\", price: 15, category: \"B\"},\n    {name: \"Item 4\", price: 25, category: \"B\"}\n  ],\n  message: input.message\n};",
          "timeout": 5000
        }
      }
    },
    {
      "id": "node_3",
      "type": "custom",
      "position": { "x": 500, "y": 100 },
      "data": {
        "type": "aggregate",
        "label": "Sum Prices",
        "category": "data",
        "icon": "Code2",
        "config": {
          "operation": "sum",
          "field": "price",
          "groupBy": ""
        }
      }
    },
    {
      "id": "node_4",
      "type": "custom",
      "position": { "x": 700, "y": 100 },
      "data": {
        "type": "log_output",
        "label": "Log Sum",
        "category": "output",
        "icon": "FileOutput",
        "config": {
          "message": "Total sum: {{input}}",
          "level": "info"
        }
      }
    }
  ],
  "edges": [
    {
      "id": "edge_1",
      "source": "node_1",
      "target": "node_2"
    },
    {
      "id": "edge_2",
      "source": "node_2",
      "target": "node_3"
    },
    {
      "id": "edge_3",
      "source": "node_3",
      "target": "node_4"
    }
  ],
  "viewport": { "x": 0, "y": 0, "zoom": 1 }
}
