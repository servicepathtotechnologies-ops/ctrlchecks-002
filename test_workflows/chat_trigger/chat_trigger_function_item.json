{
  "name": "Chat Trigger - Function Item Node Test",
  "description": "Test workflow for chat trigger with Function Item node - processes each array item",
  "nodes": [
    {
      "id": "node_1",
      "type": "custom",
      "position": { "x": 100, "y": 100 },
      "data": {
        "type": "chat_trigger",
        "label": "Chat Trigger",
        "category": "triggers",
        "icon": "MessageSquare",
        "config": {
          "triggerMode": "on_message_received"
        }
      }
    },
    {
      "id": "node_2",
      "type": "custom",
      "position": { "x": 300, "y": 100 },
      "data": {
        "type": "javascript",
        "label": "Create Array",
        "category": "data",
        "icon": "Code",
        "config": {
          "code": "return {\n  items: [\n    {id: 1, name: \"Item 1\", value: 10},\n    {id: 2, name: \"Item 2\", value: 20},\n    {id: 3, name: \"Item 3\", value: 30}\n  ],\n  message: input.message\n};",
          "timeout": 5000
        }
      }
    },
    {
      "id": "node_3",
      "type": "custom",
      "position": { "x": 500, "y": 100 },
      "data": {
        "type": "function_item",
        "label": "Process Each Item",
        "category": "data",
        "icon": "Repeat",
        "config": {
          "code": "return {\n  ...item,\n  processed: true,\n  doubled: item.value * 2,\n  index: index\n};",
          "timeout": 5000
        }
      }
    },
    {
      "id": "node_4",
      "type": "custom",
      "position": { "x": 700, "y": 100 },
      "data": {
        "type": "log_output",
        "label": "Log Processed",
        "category": "output",
        "icon": "FileOutput",
        "config": {
          "message": "Processed items: {{input.items}}",
          "level": "info"
        }
      }
    }
  ],
  "edges": [
    {
      "id": "edge_1",
      "source": "node_1",
      "target": "node_2"
    },
    {
      "id": "edge_2",
      "source": "node_2",
      "target": "node_3"
    },
    {
      "id": "edge_3",
      "source": "node_3",
      "target": "node_4"
    }
  ],
  "viewport": { "x": 0, "y": 0, "zoom": 1 }
}
