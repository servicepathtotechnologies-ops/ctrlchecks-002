{
  "name": "Chat Trigger - Code/JavaScript Node Test",
  "description": "Test workflow for chat trigger with JavaScript/Code node - executes custom JavaScript",
  "nodes": [
    {
      "id": "node_1",
      "type": "custom",
      "position": { "x": 100, "y": 100 },
      "data": {
        "type": "chat_trigger",
        "label": "Chat Trigger",
        "category": "triggers",
        "icon": "MessageSquare",
        "config": {
          "triggerMode": "on_message_received"
        }
      }
    },
    {
      "id": "node_2",
      "type": "custom",
      "position": { "x": 300, "y": 100 },
      "data": {
        "type": "javascript",
        "label": "Process with Code",
        "category": "data",
        "icon": "Code",
        "config": {
          "code": "const words = input.message.split(' ');\nconst wordCount = words.length;\nconst reversed = input.message.split('').reverse().join('');\nreturn {\n  original: input.message,\n  wordCount: wordCount,\n  reversed: reversed,\n  words: words\n};",
          "timeout": 5000
        }
      }
    },
    {
      "id": "node_3",
      "type": "custom",
      "position": { "x": 500, "y": 100 },
      "data": {
        "type": "log_output",
        "label": "Log Result",
        "category": "output",
        "icon": "FileOutput",
        "config": {
          "message": "Processed: {{input.original}} | Words: {{input.wordCount}} | Reversed: {{input.reversed}}",
          "level": "info"
        }
      }
    }
  ],
  "edges": [
    {
      "id": "edge_1",
      "source": "node_1",
      "target": "node_2"
    },
    {
      "id": "edge_2",
      "source": "node_2",
      "target": "node_3"
    }
  ],
  "viewport": { "x": 0, "y": 0, "zoom": 1 }
}
