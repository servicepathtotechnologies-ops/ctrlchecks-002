{
  "name": "Chat Trigger - Switch Node Test",
  "description": "Test workflow for chat trigger with Switch node - routes messages based on status",
  "nodes": [
    {
      "id": "node_1",
      "type": "custom",
      "position": { "x": 100, "y": 100 },
      "data": {
        "type": "chat_trigger",
        "label": "Chat Trigger",
        "category": "triggers",
        "icon": "MessageSquare",
        "config": {
          "triggerMode": "on_message_received"
        }
      }
    },
    {
      "id": "node_2",
      "type": "custom",
      "position": { "x": 300, "y": 100 },
      "data": {
        "type": "set",
        "label": "Set Status",
        "category": "data",
        "icon": "Edit",
        "config": {
          "fields": "{\"status\": \"{{input.message}}\"}"
        }
      }
    },
    {
      "id": "node_3",
      "type": "custom",
      "position": { "x": 500, "y": 100 },
      "data": {
        "type": "switch",
        "label": "Switch by Status",
        "category": "logic",
        "icon": "GitMerge",
        "config": {
          "expression": "{{input.status}}",
          "cases": "[{\"value\": \"help\", \"label\": \"Help\"}, {\"value\": \"order\", \"label\": \"Order\"}, {\"value\": \"support\", \"label\": \"Support\"}]"
        }
      }
    },
    {
      "id": "node_4",
      "type": "custom",
      "position": { "x": 700, "y": 50 },
      "data": {
        "type": "log_output",
        "label": "Log Help",
        "category": "output",
        "icon": "FileOutput",
        "config": {
          "message": "Help branch: {{input.message}}",
          "level": "info"
        }
      }
    },
    {
      "id": "node_5",
      "type": "custom",
      "position": { "x": 700, "y": 100 },
      "data": {
        "type": "log_output",
        "label": "Log Order",
        "category": "output",
        "icon": "FileOutput",
        "config": {
          "message": "Order branch: {{input.message}}",
          "level": "info"
        }
      }
    },
    {
      "id": "node_6",
      "type": "custom",
      "position": { "x": 700, "y": 150 },
      "data": {
        "type": "log_output",
        "label": "Log Support",
        "category": "output",
        "icon": "FileOutput",
        "config": {
          "message": "Support branch: {{input.message}}",
          "level": "info"
        }
      }
    }
  ],
  "edges": [
    {
      "id": "edge_1",
      "source": "node_1",
      "target": "node_2"
    },
    {
      "id": "edge_2",
      "source": "node_2",
      "target": "node_3"
    },
    {
      "id": "edge_3",
      "source": "node_3",
      "target": "node_4",
      "sourceHandle": "help"
    },
    {
      "id": "edge_4",
      "source": "node_3",
      "target": "node_5",
      "sourceHandle": "order"
    },
    {
      "id": "edge_5",
      "source": "node_3",
      "target": "node_6",
      "sourceHandle": "support"
    }
  ],
  "viewport": { "x": 0, "y": 0, "zoom": 1 }
}
