{
  "name": "Chat Trigger - Instagram Node Test",
  "description": "Test workflow for chat trigger with Instagram node - posts to Instagram",
  "nodes": [
    {
      "id": "node_1",
      "type": "custom",
      "position": { "x": 100, "y": 100 },
      "data": {
        "type": "chat_trigger",
        "label": "Chat Trigger",
        "category": "triggers",
        "icon": "MessageSquare",
        "config": {
          "triggerMode": "on_message_received"
        }
      }
    },
    {
      "id": "node_2",
      "type": "custom",
      "position": { "x": 300, "y": 100 },
      "data": {
        "type": "set",
        "label": "Prepare Post",
        "category": "data",
        "icon": "Edit",
        "config": {
          "fields": "{\"imageUrl\": \"https://example.com/image.jpg\", \"caption\": \"Chat message: {{input.message}}\"}"
        }
      }
    },
    {
      "id": "node_3",
      "type": "custom",
      "position": { "x": 500, "y": 100 },
      "data": {
        "type": "instagram",
        "label": "Post to Instagram",
        "category": "social_media",
        "icon": "Image",
        "config": {
          "accessToken": "YOUR_INSTAGRAM_ACCESS_TOKEN",
          "accountId": "YOUR_INSTAGRAM_ACCOUNT_ID",
          "operation": "create_image_post",
          "imageUrl": "{{input.imageUrl}}",
          "caption": "{{input.caption}}"
        }
      }
    },
    {
      "id": "node_4",
      "type": "custom",
      "position": { "x": 700, "y": 100 },
      "data": {
        "type": "log_output",
        "label": "Log Posted",
        "category": "output",
        "icon": "FileOutput",
        "config": {
          "message": "Instagram post created: {{input}}",
          "level": "info"
        }
      }
    }
  ],
  "edges": [
    {
      "id": "edge_1",
      "source": "node_1",
      "target": "node_2"
    },
    {
      "id": "edge_2",
      "source": "node_2",
      "target": "node_3"
    },
    {
      "id": "edge_3",
      "source": "node_3",
      "target": "node_4"
    }
  ],
  "viewport": { "x": 0, "y": 0, "zoom": 1 }
}
