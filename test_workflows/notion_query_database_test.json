{
  "name": "Notion Test - Query Database",
  "description": "Query a Notion database and filter results. Uses OAuth from header automatically.",
  "nodes": [
    {
      "id": "node_1",
      "type": "custom",
      "position": { "x": 100, "y": 100 },
      "data": {
        "type": "manual_trigger",
        "label": "Start",
        "category": "triggers",
        "icon": "Play",
        "config": {}
      }
    },
    {
      "id": "node_2",
      "type": "custom",
      "position": { "x": 300, "y": 100 },
      "data": {
        "type": "notion",
        "label": "Query Database",
        "category": "database",
        "icon": "Database",
        "config": {
          "resource": "database",
          "operation": "query",
          "databaseId": "YOUR_DATABASE_ID_HERE",
          "query": {
            "filter": {
              "property": "Status",
              "select": {"equals": "To Do"}
            },
            "sorts": [{"property": "Created", "direction": "descending"}]
          },
          "returnAll": false,
          "pageSize": 10
        }
      }
    },
    {
      "id": "node_3",
      "type": "custom",
      "position": { "x": 500, "y": 100 },
      "data": {
        "type": "log_output",
        "label": "View Results",
        "category": "output",
        "icon": "FileOutput",
        "config": {
          "message": "Found {{$json.results.length}} tasks with status 'To Do'",
          "level": "info"
        }
      }
    }
  ],
  "edges": [
    {
      "id": "edge_1",
      "source": "node_1",
      "target": "node_2"
    },
    {
      "id": "edge_2",
      "source": "node_2",
      "target": "node_3"
    }
  ],
  "viewport": { "x": 0, "y": 0, "zoom": 1 }
}
