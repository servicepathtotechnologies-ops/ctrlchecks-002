# Vercel ignore file - files to exclude from deployment

# Development files
.env.local
.env.*.local
*.local

# Test files
**/*.test.ts
**/*.test.tsx
**/*.spec.ts
**/*.spec.tsx
test_workflows/
# Exclude model-testing config files, but keep source components
model-testing/**/*.json
model-testing/**/test-config.json
# But NOT the actual component files - they're needed for the app
# model-testing/ is now included for production

# Documentation
docs/
*.md
!README.md

# Scripts (not needed in production)
scripts/
*.ps1
*.sh
*.bat

# Docker files
Dockerfile*
docker-compose*.yml
nginx*.conf

# Cloudflare tunnel config
cloudflare-tunnel-config.yml

# SQL migrations (handled separately)
sql_migrations/
# Supabase config folder (root level only, not src/integrations/supabase/)
/supabase/

# IDE files
.vscode/
.idea/
*.swp
*.swo

# OS files
.DS_Store
Thumbs.db

# Logs
*.log
logs/

# Temporary files
*.tmp
*.temp
.cache/

# Build artifacts (will be generated)
dist/
build/
